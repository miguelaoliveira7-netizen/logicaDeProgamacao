algoritmo "Nome do Programa"

var
usuario , senha : caractere
inicio

escreval("Digite o usuario")
leia(usuario)

escreval("Digite a senha")
leia(senha)

se (usuario = "ADM") e (senha = "ADM") entao
    escreval("Seja bem vindo(a) ADM")
senao
    escreval("Usuario ou senha incorretos!")
fimSe

fimalgoritmo